                    "multipart/form-data"
                ],
                "produces": [
                    "application/json",
                    "application/json"
                ],
                "tags": [
                    "Producer-External-API-V1"
                ],
                "parameters": [
                    {
                        "type": "string",
                        "example": "Hello {{.name}}, thank you for shopping with us! Please confirm your phone number ",
                        "name": "message_template",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "format": "date-time",
                        "example": "2024-09-24T16:57:00+08:00 defined by RFC3339, section5.6",
                        "name": "schedule_time",
                        "in": "query"
                    },
                    {
                        "type": "file",
                        "description": "CSV file",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/producer_pkg_utils.ResponseWithRequestId"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/producer_pkg_utils.ResponseWithRequestId"
                        }
                    }
                }
            }
        },
        "/api/v1/messages": {
            "post": {
